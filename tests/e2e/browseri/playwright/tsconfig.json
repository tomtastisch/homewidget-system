{
    "compilerOptions": {
        "target": "ES2015",
        "module": "esnext",
        "lib": [
            "ES2015",
            "DOM"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "bundler",
        "types": [
            "node",
            "@playwright/test"
        ]
    },
    "include": [
        "**/*.ts"
    ]
}
